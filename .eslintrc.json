{
  "env": {
    "browser": true,
    "es2022": true
  },
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off",
    "semi": ["error", "always"],
    "quotes": ["error", "single", { "avoidEscape": true }],
    "indent": ["error", 4],
    "no-eval": "error",
    "no-implied-eval": "error",
    "no-new-func": "error"
  },
  "globals": {
    "Cal": "readonly"
  }
}
